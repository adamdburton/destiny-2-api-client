{
  "name": "adamdburton/destiny-2-api-client",
  "description": "PHP API client for Destiny 2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "adamdburton",
      "email": "adam@burt0n.net"
    }
  ],
  "require": {
    "ext-zip": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "php-http/discovery": "^1.6",
    "php-http/client-implementation": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "php-http/mock-client": "^1"
  },
  "autoload": {
    "psr-4": {
      "AdamDBurton\\Destiny2ApiClient\\": "src"
    },
    "classmap": [
      "tests"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdamDBurton\\Destiny2ApiClient\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "Destiny": "AdamDBurton\\Destiny2ApiClient\\Laravel\\Facade"
      }
    }
  }
}
